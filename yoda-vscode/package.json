{
    "name": "yoda-vscode",
    "displayName": "YodaOS",
    "description": "YodaOS VSCode plugin",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.29.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.sayHello"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [{
            "command": "extension.sayHello",
            "title": "Hello World"
        }]
    },
    "scripts": {
        "vscode:prepublish": "yarn run build",
        "postinstall": "npx vscode-install",
        "build": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "test": "yarn run build"
    },
    "dependencies": {
      "yoda-platform-lib": "0.0.1"
    },
    "devDependencies": {
        "typescript": "^3.1.4",
        "vscode": "^1.1.25",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    }
}
